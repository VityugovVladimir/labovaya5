{
  "name": "gh-pages-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vityugovvladimir.github.io/labovaya5",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run save-json-api && shx cp build/index.html build/404.html",
    "save-json-api": "node ./src/server/scripts/save-json-api.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "shx rm -rf build && npm run build",
    "deploy": "gh-pages -d build -b master",
    "server": "json-server --watch src/server/db/data.json --port 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "json-server": "^1.0.0-beta.3",
    "node-fs": "^0.1.7",
    "shx": "^0.3.3"
  }
}
